    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^17.7.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.14",
    "@vite-pwa/nuxt": "^0.10.8",
    "autoprefixer": "^10.4.21",
    "mongodb": "^6.5.0",
